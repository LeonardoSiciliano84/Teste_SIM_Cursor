# Banco de Dados PostgreSQL
DATABASE_URL=postgresql://usuario:senha@host:porta/database

# Segurança da Sessão
SESSION_SECRET=sua_chave_secreta_super_forte_aqui

# Configurações do Banco (Auto-preenchidas pelo Replit)
PGHOST=localhost
PGPORT=5432
PGUSER=usuario
PGPASSWORD=senha
PGDATABASE=felka_db

# Email (SendGrid - Opcional)
SENDGRID_API_KEY=sua_chave_api_sendgrid_aqui

# Ambiente
NODE_ENV=development